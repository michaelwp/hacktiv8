<%- include partials/head %>
<%- include partials/menu.ejs %>
<h3><%= message %></h3>
<table>
    <tr>
        <th>#</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>User Name</th>
        <th>Type</th>
        <th>Age</th>
        <th>Exp</th>
    </tr>
    <% dataMember.forEach((e, index) => { %>
        <tr>
            <td><%= index + 1 %></td>
            <td><a href="/members/<%= e.dataValues.id %>"><%= e.dataValues.firstName %></a></td>
            <td><%= e.dataValues.lastName %></td>
            <td><%= e.dataValues.username %></td>
            <td style="background-color: <%= memberTypeColor(e.dataValues.type) %>"><%= e.dataValues.type %></td>
            <td><%= e.dataValues.age %></td>
            <td><%= e.dataValues.exp %></td>
        </tr>
    <% }) %>
</table>
<%- include partials/foot %>