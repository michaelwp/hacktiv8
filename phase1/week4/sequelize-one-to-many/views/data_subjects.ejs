<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"
          integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" type="text/css" href="../style.css">
    <title>Data Subjects</title>
</head>
<body>
<div class="jumbotron">
    <p>
        <a class="btn btn-primary" href="/subjects/add" role="button">Add New</a>
    </p>
    <table class="table">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">id</th>
            <th scope="col">Subject Name</th>
            <th scope="col">Teacher</th>
        </tr>
        </thead>
        <tbody>
        <% dataSubjects.forEach((e, index) => { %>
            <tr>
                <th scope="row"><%= index + 1 %></th>
                <td><%= e.dataValues.id %></td>
                <td><%= e.dataValues.subject_name %></td>
                <td>
                    <%= e.dataValues.teacher.dataValues.first_name %>
                    <%= e.dataValues.teacher.dataValues.last_name %>
                </td>
                <td>
                    <a class="btn btn-success" href="/subjects/edit/<%= e.dataValues.id %>" role="button">Edit</a>
                    <a class="btn btn-danger" href="/subjects/delete/<%= e.dataValues.id %>" role="button">Delete</a>
                </td>
            </tr>
        <% }) %>
        </tbody>
    </table>
</div>
</body>
</html>